<?xml version="1.0" encoding="UTF-8"?>
<project xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd" xmlns="http://maven.apache.org/POM/4.0.0"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <modelVersion>4.0.0</modelVersion>
  <parent>
    <groupId>com.github.dapeng-soa</groupId>
    <artifactId>dapeng-springboot-project</artifactId>
    <version>2.2.0-SNAPSHOT</version>
  </parent>
  <groupId>com.github.dapeng-soa</groupId>
  <artifactId>dapeng-springboot-demo</artifactId>
  <version>2.2.0-SNAPSHOT</version>
  <packaging>pom</packaging>
  <licenses>
    <license>
      <name>The Apache Software License, Version 2.0</name>
      <url>http://www.apache.org/licenses/LICENSE-2.0.txt</url>
    </license>
  </licenses>
  <modules>
    <module>hello-api</module>
    <module>hello-springboot-provider</module>
    <module>hello-test-client</module>
  </modules>
  <properties>
    <java.version>1.8</java.version>
    <dapeng.version>2.2.0-SNAPSHOT</dapeng.version>
  </properties>
  <build>
    <plugins>
      <plugin>
        <artifactId>maven-compiler-plugin</artifactId>
        <configuration>
          <source>${java.version}</source>
          <target>${java.version}</target>
          <encoding>utf-8</encoding>
        </configuration>
      </plugin>
    </plugins>
  </build>
</project>
